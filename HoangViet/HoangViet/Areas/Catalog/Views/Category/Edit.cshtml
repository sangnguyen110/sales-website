@model HoangViet.Models.Catalog.Category

@{
    ViewBag.Title = "Edit";
}

<div class="box-header">
    <h3 class="box-title">
        Cập nhật danh mục hàng hóa
    </h3>
</div>


@using (Html.BeginForm())
{
